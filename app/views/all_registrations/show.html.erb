<%- model_class = AllRegistration -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:roll_no) %>:</strong></dt>
  <dd><%= @all_registration.roll_no %></dd>
  <dt><strong><%= model_class.human_attribute_name(:reg_no) %>:</strong></dt>
  <dd><%= @all_registration.reg_no %></dd>
  <dt><strong><%= model_class.human_attribute_name(:first_name) %>:</strong></dt>
  <dd><%= @all_registration.first_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:last_name) %>:</strong></dt>
  <dd><%= @all_registration.last_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:father_name) %>:</strong></dt>
  <dd><%= @all_registration.father_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ph_no) %>:</strong></dt>
  <dd><%= @all_registration.ph_no %></dd>
  <dt><strong><%= model_class.human_attribute_name(:gender) %>:</strong></dt>
  <dd><%= @all_registration.gender %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dob) %>:</strong></dt>
  <dd><%= @all_registration.dob %></dd>
  <dt><strong><%= model_class.human_attribute_name(:age) %>:</strong></dt>
  <dd><%= @all_registration.age %></dd>
  <dt><strong><%= model_class.human_attribute_name(:address) %>:</strong></dt>
  <dd><%= @all_registration.address %></dd>
  <dt><strong><%= model_class.human_attribute_name(:city) %>:</strong></dt>
  <dd><%= @all_registration.city %></dd>
  <dt><strong><%= model_class.human_attribute_name(:state) %>:</strong></dt>
  <dd><%= @all_registration.state %></dd>
  <dt><strong><%= model_class.human_attribute_name(:country) %>:</strong></dt>
  <dd><%= @all_registration.country %></dd>
  <dt><strong><%= model_class.human_attribute_name(:pincode) %>:</strong></dt>
  <dd><%= @all_registration.pincode %></dd>
  <dt><strong><%= model_class.human_attribute_name(:hall_ticket) %>:</strong></dt>
  <dd><%= @all_registration.hall_ticket %></dd>
  <dt><strong><%= model_class.human_attribute_name(:category) %>:</strong></dt>
  <dd><%= @all_registration.category %></dd>
  <dt><strong><%= model_class.human_attribute_name(:group) %>:</strong></dt>
  <dd><%= @all_registration.group %></dd>
  <dt><strong><%= model_class.human_attribute_name(:year) %>:</strong></dt>
  <dd><%= @all_registration.year %></dd>
  <dt><strong><%= model_class.human_attribute_name(:reg_fee) %>:</strong></dt>
  <dd><%= @all_registration.reg_fee %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              all_registrations_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_all_registration_path(@all_registration), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              all_registration_path(@all_registration),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
