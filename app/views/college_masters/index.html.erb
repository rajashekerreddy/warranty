<%- model_class = CollegeMaster -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:college_name) %></th>
      <th><%= model_class.human_attribute_name(:college_code) %></th>
      <th><%= model_class.human_attribute_name(:category) %></th>
      <th><%= model_class.human_attribute_name(:address) %></th>
      <th><%= model_class.human_attribute_name(:ph_no) %></th>
      <th><%= model_class.human_attribute_name(:mobile_no) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @college_masters.each do |college_master| %>
      <tr>
        <td><%= link_to college_master.id, college_master_path(college_master) %></td>
        <td><%= college_master.college_name %></td>
        <td><%= college_master.college_code %></td>
        <td><%= college_master.category %></td>
        <td><%= college_master.address %></td>
        <td><%= college_master.ph_no %></td>
        <td><%= college_master.mobile_no %></td>
        <td><%=l college_master.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_college_master_path(college_master), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      college_master_path(college_master),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_college_master_path,
            :class => 'btn btn-primary' %>
